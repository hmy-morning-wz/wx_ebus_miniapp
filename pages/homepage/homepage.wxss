.container-homepage {
  min-height: 100vh;
  background-color: #F7F7FA;
}
.userinfo {
  width: 750rpx;
  height: 176rpx;
  display: flex;
  align-items: center;
  background-color: #ffffff;
  font-size: 34rpx;
  font-family: PingFangSC, PingFangSC-Semibold;
  font-weight: 600;
  color: #000000;
  line-height: 48rpx;
}
.userinfo image {
  display: inline-block;
  width: 112rpx;
  height: 112rpx;
  border-radius: 100%;
  margin-left: 32rpx;
  margin-right: 24rpx;
}
.menu-line {
  height: 192rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #ffffff;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  color: #000000;
  line-height: 40rpx;
}
.menu-banner {
  width: 373rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.menu-banner image {
  display: inline-block;
  width: 56rpx;
  height: 56rpx;
  margin-bottom: 24rpx;
}
.gray-column {
  width: 2rpx;
  height: 64rpx;
  background: rgba(0,0,0,0.1);
}